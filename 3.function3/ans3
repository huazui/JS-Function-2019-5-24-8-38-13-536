function compare(v1,v2){
	if(v1<v2){
		return -1;
	}
	else if(v1>v2){
		return 1;
	}
	else{
		return 0;
	}
}
function alphabetSort(message){
  var str=[];
  for(var i=0;i<message.length;i++)
	  str[i]=message[i].toLowerCase();
	  str.sort(compare);
	var ss=str.join("");
	return ss;
}
alphabetSort('hello'); 
